{
    "manifest_version": 3,
    "name": "Productifi",
    "version": "1.0",
    "description": "Block or save urls on the internet",
    "action":{
      "default_popup":"main-page.html"
    },
    "icons": {
      "16": "./icons/sprout16.png",
      "32": "./icons/sprout32.png",
      "48": "./icons/sprout48.png",
      "128": "./icons/sprout128.png"
    },
    "permissions": [
      "tabs",
      "activeTab"
  ]
  }